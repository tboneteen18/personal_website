<div class="container">
  <h1 class = "text-center">List of rounds in the tournament</h1>
  <div class="container">
    <% @round.each do |round| %>
      <ul>
        <li> <%= link_to round.number, tournament_round_result_index_path(round.tournament, round) %> </li>
      </ul>
    <% end %>
    <%= link_to "add a round", tournament_round_new_path %>
  </div>
</div>
<%= link_to tournament_index_path do%>
<i class="fa fa-times-circle fa-3x"></i>
<% end %>
